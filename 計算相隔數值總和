import java.util.Scanner;

public class Main{

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		while (true) {
			int s = sc.nextInt();
			int e = sc.nextInt();
			int d = sc.nextInt();
			int a = 0;
			if (e > s) {
				for (int i = s; i <= e; i = i + d) {
					a += i;
				}
				System.out.println(a);
			}
			if (e < s) {
				System.out.println("Error");
			}
		}
	}
}
